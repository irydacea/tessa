		"GUILD ID 1": {
			"event_log": "CHANNEL ID FOR MODERATION EVENT LOGGING", // default: 0 (no logging)
			"CHANNEL ID 1": {
				"auto_reactions": true, // default: false
				"moderate_reactions": false // default: false
			},
			"CHANNEL ID 2": {
				"auto_reactions": true,
				"moderate_reactions": true
			}
		},
		"GUILD ID 2": {
			"CHANNEL ID 3": {
				"moderate_reactions": true
			}
		}
	},

	//
	// Set of reaction emojis to add to messages
